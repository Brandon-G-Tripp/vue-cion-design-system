{
  "name": "vue-ds",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all --parallel theo:onchange theo serve",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "theo": "theo ./src/system/tokens/tokens.yml --transform web --format map.scss,scss,raw.json,json --dest ./src/system/assets/tokens",
    "theo:onchange": "onchange \"./src/system/tokens/*.yml\" -- npm run theo"
  },
  "dependencies": {
    "vue": "^2.5.17",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@babel/standalone": "^7.0.0-beta.56",
    "@vue/cli-plugin-babel": "^3.0.0-rc.12",
    "@vue/cli-plugin-eslint": "^3.0.0-rc.12",
    "@vue/cli-service": "^3.0.0-rc.12",
    "@vue/eslint-config-prettier": "^3.0.0-rc.12",
    "codemirror": "^5.39.2",
    "npm-run-all": "^4.1.3",
    "onchange": "^4.1.0",
    "theo": "^8.0.0-beta.2",
    "vue-docgen-api": "^2.3.13",
    "vue-markdown-loader": "^2.4.1",
    "vue-template-compiler": "^2.5.17",
    "vuep": "^0.8.0"
  }
}
